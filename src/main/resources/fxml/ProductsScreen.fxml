<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sdabyd2.javafx.orders.controller.ProductsController">
   <children>
      <VBox prefHeight="400.0" prefWidth="600.0">
         <children>
            <HBox prefHeight="38.0" prefWidth="600.0">
               <children>
                  <Label prefHeight="27.0" prefWidth="448.0" text="PRODUKTY">
                     <font>
                        <Font name="System Bold" size="18.0" />
                     </font>
                     <HBox.margin>
                        <Insets bottom="5.0" left="10.0" right="5.0" top="10.0" />
                     </HBox.margin>
                  </Label>
                  <Button fx:id="buttonSave" mnemonicParsing="false" onAction="#onActionButtonSave" text="Zapisz">
                     <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="10.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="buttonLoad" mnemonicParsing="false" onAction="#onActionButtonLoad" text="Odczytaj">
                     <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="10.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
            <HBox prefHeight="21.0" prefWidth="600.0">
               <children>
                  <Label prefHeight="17.0" prefWidth="446.0" text="Nazwa">
                     <HBox.margin>
                        <Insets left="10.0" right="5.0" />
                     </HBox.margin>
                  </Label>
                  <Label prefHeight="17.0" prefWidth="62.0" text="Cena jedn.">
                     <HBox.margin>
                        <Insets left="5.0" right="5.0" />
                     </HBox.margin>
                  </Label>
               </children>
            </HBox>
            <HBox fx:id="hBoxProducts" prefHeight="351.0" prefWidth="580.0">
               <children>
                  <Group fx:id="groupProducts" />
               </children>
               <VBox.margin>
                  <Insets left="10.0" right="10.0" />
               </VBox.margin>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button fx:id="buttonAddProduct" mnemonicParsing="false" onAction="#onActionButtonAddItem" prefHeight="25.0" prefWidth="101.0" text="Dodaj produkt">
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="5.0" top="10.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="buttonEditProduct" disable="true" mnemonicParsing="false" onAction="#onActionButtonEditItem" prefHeight="25.0" prefWidth="122.0" text="Modyfikuj produkt">
                     <HBox.margin>
                        <Insets bottom="10.0" left="5.0" right="5.0" top="10.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="buttonDeleteProduct" disable="true" mnemonicParsing="false" onAction="#onActionButtonDeleteItem" prefHeight="25.0" prefWidth="96.0" text="UsuÅ„ produkt">
                     <HBox.margin>
                        <Insets bottom="10.0" left="5.0" right="180.0" top="10.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="buttonClose" mnemonicParsing="false" onAction="#onActionButtonClose" text="Zamknij">
                     <HBox.margin>
                        <Insets bottom="10.0" left="5.0" right="10.0" top="10.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
